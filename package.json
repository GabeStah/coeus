{
  "name": "coeus",
  "private": true,
  "description": "Titan of insight and intellect.",
  "version": "1.7.0",
  "author": "Gabe Wyatt <gabe@solarixdigital.com>",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@babel/register": "^7.8.6",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/gitlab": "^6.0.4",
    "@semantic-release/npm": "^7.0.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/convict": "^5.2.1",
    "@types/eslint": "^7.2.2",
    "@types/faker": "^5.1.0",
    "@types/http-errors": "^1.8.0",
    "@types/mongodb": "^3.5.26",
    "@types/object-hash": "^1.3.3",
    "@types/pino": "^6.3.1",
    "@types/pump": "^1.1.0",
    "@types/split2": "^2.1.6",
    "@types/tap": "^14.10.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "autocannon": "^6.4.0",
    "bson-ext": "^2.0.5",
    "commitizen": "^4.1.2",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.7.0",
    "faker": "^5.1.0",
    "fastify-caching": "^6.0.1",
    "fastify-compress": "^3.3.0",
    "fastify-cors": "^4.1.0",
    "fastify-oas": "^3.0.3",
    "fastify-plugin": "^2.3.3",
    "fastify-swagger": "^3.3.0",
    "husky": "^4.2.5",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "tap": "^14.10.8",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.2",
    "typescript-json-schema": "^0.43.0"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc && yarn run lint",
    "commit": "git-cz",
    "docker:mongo": "docker run -p 27017:27017 --name mongo mongo",
    "lint": "tsc --noEmit && eslint \"src/**/*.{js,ts}\" --quiet --fix",
    "prebuild": "rimraf dist",
    "start": "node dist/server.js",
    "start:development": "cross-env NODE_ENV=development node dist/server.js",
    "start:testing": "cross-env NODE_ENV=testing node dist/server.js",
    "start:production": "cross-env NODE_ENV=production node dist/server.js",
    "test": "tap -t 0 --node-arg=--require=tsconfig-paths/register \"src/**/*.test.ts\"",
    "test:coverage": "tap -t 0 --coverage-report=lcov --node-arg=--require=tsconfig-paths/register \"src/**/*.test.ts\"",
    "test:current": "tap -t 0 --node-arg=--require=tsconfig-paths/register \"src/routes/data/update.test.ts\"",
    "test:debug": "tap -t 0 --node-arg=--require=tsconfig-paths/register --debug \"src/**/*.test.ts\"",
    "test:watch": "tap --reporter=list --watch --node-arg=--require=tsconfig-paths/register \"src/**/*.test.ts\"",
    "test:watch:debug": "tap --reporter=list --watch --node-arg=--require=tsconfig-paths/register --debug \"src/**/*.test.ts\"",
    "watch": "tsc -w",
    "watch:server": "nodemon dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.solarixdigital.com/solarix/core/soldata/coeus"
  },
  "bugs": {
    "url": "https://gitlab.solarixdigital.com/solarix/core/soldata/coeus/issues"
  },
  "main": "dist/server.js",
  "files": [
    "dist"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "outputDirectory": "dist"
  },
  "dependencies": {
    "@types/json5": "^0.0.30",
    "@types/lodash": "^4.14.161",
    "@types/nodemailer": "^6.4.0",
    "aws-sdk": "^2.751.0",
    "bcryptjs": "^2.4.3",
    "convict": "^6.0.0",
    "fastify": "^3.3.0",
    "fastify-jwt": "^2.1.3",
    "fastify-rate-limit": "git://github.com/GabeStah/fastify-rate-limit/#add-fastify-hook-method-setting",
    "fastify-request-context": "^2.1.1",
    "http-errors": "^1.8.0",
    "lodash": "^4.17.20",
    "mongodb": "^3.6.0",
    "nodemailer": "^6.4.11",
    "object-hash": "^2.0.3"
  },
  "_moduleAliases": {
    "config": "dist/config/index.js",
    "src": "dist"
  }
}
