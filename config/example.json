{
  "db": {
    mongo: {
      uri: "mongodb+srv://<name>:<password>@<domain>/"
    },
    thresholds: {
      limit: {
        base: 20,
        minimum: 1,
        maximum: 100
      }
    }
  },
  "port": 8000,
  "routes": {
    "root": "http://127.0.0.1:8000"
  },
  "security": {
    "ssl": {
      "enabled": false
    }
  },
  "services": {
    "aws": {
      "cloudwatch": {
        "logStreamName": "coeus/base/development/log"
      }
    }
  }
}
